  "scripts": {
    "start": "npm-run-all -p dev-scripts react-start watch-css",
    "deploy": "npm-run-all -s dev-scripts build deploy:firebase",
    "deploy:firebase": "firebase deploy",
    "story": "getstorybook",
    "dev-scripts": "npm-run-all -p make-lists build-css",
    "build-scripts": "npm-run-all -p make-lists build-css build",
    "make-lists": "node ./get-images.js",
    "react-start": "react-scripts start",
    "build-css": "node-sass src/ -o src/  -r --output-style compress --sourcemap true --source-map-embed ---source-map-contents css --include-path ./node_modules",
    "watch-css": "node-sass src/ -o src/ -r -w --output-style compress --sourcemap true --source-map-embed --source-map-contents css --include-path ./node_modules",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0"
  }
