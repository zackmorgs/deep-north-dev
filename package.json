    "ls": "^0.2.1",
    "micro": "^9.3.3",
    "node-sass": "^4.9.3",
    "npm": "^6.4.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-feather": "^1.1.1",
    "react-helmet": "^5.2.0",
    "react-image-gallery": "^0.8.11",
    "react-phone-input-2": "^2.6.0",
    "react-photo-gallery": "^6.2.1",
    "react-responsive-carousel": "^3.1.41",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-transition": "^1.2.1",
    "react-scripts": "1.1.4",
