{
  "name": "deep-north-dev",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "gulp": "^3.9.1",
    "linebyline": "^1.3.0",
    "ls": "^0.2.1",
    "node-sass": "^4.9.3",
    "npm": "^6.4.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-responsive-carousel": "^3.1.41",
    "react-scripts": "1.1.4",
    "react-sizeme": "^2.5.2",
    "react-slick": "^0.23.1",
    "readline": "^1.3.0",
    "serve": "^9.6.0"
  },
  "scripts": {
    "start": "npm-run-all -p get-image-list build-css react-start watch-css",
    "get-image-list": "sh get-images.sh",
    "react-start": "react-scripts start",
    "build-css": "node-sass src/ -o src/  -r",
    "watch-css": "node-sass src/ -o src/ -r -w",
    "wat": "which react-scripts",
    "build": "react-scripts build",
    "build-all": "npm run-script get-image-list && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
